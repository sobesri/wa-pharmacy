{
  "openapi": "3.0.0",
  "info": {
    "title": "WA Pharmacy",
    "version": "1.0.0",
    "description": "My API Description"
  },
  "paths": {
    "/api/users": {
      "get": {
        "summary": "Get all users",
        "operationId": "getAllUsers",
        "parameters": [
          {
            "name": "searchTerm",
            "in": "query",
            "description": "Search term",
            "schema": {
              "type": "string"
            }
          },
          {
            "name": "limit",
            "in": "query",
            "description": "Search results count",
            "schema": {
              "type": "integer"
            }
          },
          {
            "name": "offset",
            "in": "query",
            "description": "Search results page",
            "schema": {
              "type": "integer"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "Successful response",
            "content": {
              "application/json": {
                "example": [
                  {
                    "id": 1,
                    "name": "John Doe",
                    "address": "123 Main St",
                    "phone": "555-1234"
                  }
                ]
              }
            }
          }
        }
      },
      "post": {
        "summary": "Create a new customer",
        "operationId": "createCustomer",
        "requestBody": {
          "required": true,
          "content": {
            "application/json": {
              "example": {
                "name": "Jane Doe",
                "address": "456 Oak St",
                "phone": "555-5678"
              }
            }
          }
        },
        "responses": {
          "201": {
            "description": "Customer created successfully",
            "content": {
              "application/json": {
                "example": {
                  "id": 2,
                  "name": "Jane Doe",
                  "address": "456 Oak St",
                  "phone": "555-5678"
                }
              }
            }
          }
        }
      }
    },
    "/api/customers/{id}": {
      "get": {
        "summary": "Get a specific customer by ID",
        "operationId": "getCustomerById",
        "parameters": [
          {
            "name": "id",
            "in": "path",
            "required": true,
            "description": "ID of the customer",
            "schema": {
              "type": "integer"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "Successful response",
            "content": {
              "application/json": {
                "example": {
                  "id": 1,
                  "name": "John Doe",
                  "address": "123 Main St",
                  "phone": "555-1234"
                }
              }
            }
          }
        }
      },
      "put": {
        "summary": "Update a specific customer by ID",
        "operationId": "updateCustomer",
        "parameters": [
          {
            "name": "id",
            "in": "path",
            "required": true,
            "description": "ID of the customer",
            "schema": {
              "type": "integer"
            }
          }
        ],
        "requestBody": {
          "required": true,
          "content": {
            "application/json": {
              "example": {
                "name": "Updated Name",
                "address": "Updated Address",
                "phone": "555-9876"
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "Customer updated successfully",
            "content": {
              "application/json": {
                "example": {
                  "id": 1,
                  "name": "Updated Name",
                  "address": "Updated Address",
                  "phone": "555-9876"
                }
              }
            }
          }
        }
      },
      "delete": {
        "summary": "Delete a specific customer by ID",
        "operationId": "deleteCustomer",
        "parameters": [
          {
            "name": "id",
            "in": "path",
            "required": true,
            "description": "ID of the customer",
            "schema": {
              "type": "integer"
            }
          }
        ],
        "responses": {
          "204": {
            "description": "Customer deleted successfully"
          }
        }
      }
    }
  }
}